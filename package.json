{
  "name": "@jaylouisw/n8n-nodes-twingate",
  "version": "0.1.0",
  "description": "n8n node for Twingate Zero Trust Network Access",
  "keywords": [
    "n8n-community-node-package",
    "twingate",
    "zero-trust",
    "network",
    "vpn",
    "security"
  ],
  "license": "MIT",
  "homepage": "https://github.com/Jaylouisw/n8n-nodes-twingate",
  "author": {
    "name": "Jaylouisw"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Jaylouisw/n8n-nodes-twingate.git"
  },
  "engines": {
    "node": ">=18.10"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/TwingateApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Twingate/Twingate.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^3.2.5",
    "typescript": "^5.4.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
